{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "OpenAISpanAttributes",
    "type": "object",
    "properties": {
      "service.provider": {
        "type": "string"
      },
      "url.full": {
        "type": "string"
      },
      "llm.api": {
        "type": "string"
      },
      "llm.model": {
        "type": "string"
      },
      "llm.temperature": {
        "type": "number",
        "format": "float"
      },
      "llm.top_p": {
        "type": "number",
        "format": "float"
      },
      "llm.user": {
        "type": "string"
      },
      "llm.system.fingerprint": {
        "type": "string"
      },
      "http.max.retries": {
        "type": "integer",
        "format": "int32"
      },
      "http.timeout": {
        "type": "integer",
        "format": "int64"
      },
      "llm.prompts": {
        "type": "string"
      },
      "llm.responses": {
        "type": "string"
      },
      "llm.token.counts": {
        "type": "string"
      },
      "llm.stream": {
        "type": "boolean"
      },
      "llm.encoding.format": {
        "type": "string"
      },
      "llm.dimensions" : {
        "type": "string"
      }
    },
    "required": [
      "service.provider",
      "url.full",
      "llm.api",
      "llm.model",
      "llm.prompts"
    ]
  }
  